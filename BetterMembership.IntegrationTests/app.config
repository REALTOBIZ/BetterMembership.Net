<?xml version="1.0" encoding="utf-8"?>

<configuration>
	<configSections>
		<section name="entityFramework"
		         type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=5.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"
		         requirePermission="false" />
	</configSections>
	<connectionStrings>
		<add name="DefaultConnection"
		     connectionString="Data Source=|DataDirectory|GenericProvider.sdf;Persist Security Info=False;"
		     providerName="System.Data.SqlServerCe.4.0" />
		<add name="DefaultConnection2"
		     connectionString="Data Source=|DataDirectory|GenericProvider2.sdf;Persist Security Info=False;"
		     providerName="System.Data.SqlServerCe.4.0" />
	</connectionStrings>
	<appSettings>
		<add key="enableSimpleMembership" value="false" />
	</appSettings>
	<system.web>
		<membership defaultProvider="defaultProvider">
			<providers>
				<add name="defaultProvider" type="BetterMembership.BetterMembershipProvider, BetterMembershipProvider"
				     connectionStringName="SqlServerCE"  autoInitialize="true" />
				<add name="nonDefaultProvider" type="BetterMembership.BetterMembershipProvider, BetterMembershipProvider"
				     connectionStringName="SqlServerCE2" userTableName="CustomUserTable" autoInitialize="true" />
			</providers>
		</membership>
		<roleManager enabled="true" />
	</system.web>
	<entityFramework>
		<defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlCeConnectionFactory, EntityFramework">
			<parameters>
				<parameter value="System.Data.SqlServerCe.4.0" />
			</parameters>
		</defaultConnectionFactory>
	</entityFramework>
</configuration>